<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.triosstudent.csd214_lab1_johncarlo.UserController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <TableView fx:id="userTable" prefHeight="200.0" prefWidth="525.0">
        <columns>
            <TableColumn fx:id="id" prefWidth="75.0" text="id"/>
            <TableColumn fx:id="name" prefWidth="150.0" text="name"/>
            <TableColumn fx:id="email" prefWidth="150.0" text="email"/>
            <TableColumn fx:id="dob" prefWidth="150.0" text="dob"/>
        </columns>
    </TableView>
    <Button onAction="#onLoadUsersButtonClick" text="Load Users"/>
</VBox>
